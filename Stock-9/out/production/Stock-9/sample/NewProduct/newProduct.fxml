<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="154.0" prefWidth="600.0" style="-fx-background-color: #32de84;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.NewProduct.NewProduct">
   <children>
      <JFXTextField fx:id="productName" alignment="CENTER" focusColor="#f3f6ff" labelFloat="true" layoutX="23.0" layoutY="38.0" promptText="Product Name">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="productID" alignment="CENTER" focusColor="#eef3ff" labelFloat="true" layoutX="212.0" layoutY="38.0" promptText="Product ID">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </JFXTextField>
      <JFXTextField fx:id="unitPrice" alignment="CENTER" focusColor="#eaebee" labelFloat="true" layoutX="419.0" layoutY="38.0" promptText="Unit Price">
         <font>
            <Font name="Arial Bold" size="13.0" />
         </font>
      </JFXTextField>
      <Button fx:id="cancelButton" layoutX="65.0" layoutY="89.0" mnemonicParsing="false" onAction="#cancelAction" prefHeight="35.0" prefWidth="113.0" text="Cancel">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Button>
      <Button fx:id="saveButton" layoutX="419.0" layoutY="89.0" mnemonicParsing="false" onAction="#saveOnAction" prefHeight="35.0" prefWidth="113.0" text="Save">
         <font>
            <Font name="Arial" size="13.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
